  <rule id="daSacStd" message="You may only take one Sacred Standard per Interrogator-Chaplain." summary="You may only take one Sacred Standard per Interrogator-Chaplain.">
    <set set="daStd"/>
    <test/>
    <valid><![CDATA[~ If Apocalypse ruleset is in force, ignore the rest of this rule
if (tagcount[ruleset.ro_Apoc] = 1) then
   @valid = 1
   done
endif

~ If you have more Command Squads than Independent Characters, flag an error
if (tagcount[daTagsG.SacStd] <= tagcount[daTags.IntChap]) then
   @valid = 1
endif
]]></valid>
    </rule>
  <rule id="daCmdSqd" message="You may only take one Command Squad per Independent Character." summary="You may only take one Command Squad per Independent Character.">
