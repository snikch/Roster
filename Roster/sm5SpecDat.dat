  <rule id="ia9_Barrag" message="You can take only one Artillery Barrage per 2,000 points." summary="One Artillery Barrage per 2,000 points.">
    <set set="smChTyrant"/>
    <test><![CDATA[ruleset.ro_Apoc & ruleset.smChTyrant]]></test>
    <valid><![CDATA[var rostersize as number
var barrage as number
var ratio as number

if (tagexpr[roster.size999999] = 1) then
   rostersize = roster.activesize
else     
   rostersize = roster.usersize
endif

barrage = tagcount[ia9Tags.ArtBarrage]
ratio = (rostersize / 2000)

if (barrage <= ratio) then
      @valid = 1
endif]]></valid>
    </rule>
  <rule id="ia9_ChapSu" message="Too many Chapter Support choices taken." summary="Too many Chapter support choices taken.">
    <set set="smChTyrant"/>
    <test/>
    <valid><![CDATA[~ You cannot take more Chapter Support choices than SM Cohort units
var cohort as number
var support as number

cohort = tagcount[ia9Tags.SM_Cohort]
support = tagcount[ia9Tags.ChapSup]

if (support <= cohort) then
   @valid = 1
endif
]]></valid>
    </rule>
