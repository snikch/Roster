  <option id="ia9_AuxCre" name="Guardsmen" abbrev="" category="add" priority="10" cost="0" unit="ia9_AuxCre" show="never" textcost=" ">
    <sizelimit><![CDATA[~ first get count of guns from parent
var gunCount as number
var gunCAdj as number

gunCount = tagcount[iaCrew.iaGuard]

~ heavy mortar
if (tagis[unitid.iaDkHvyMor] = 1) then
~ now triple for 3 guardsmen per gun
gunCount = gunCount * 3
if (tagis[ruleset.ig_sr_deat] = 1) then
@maximum = gunCount * 1.67
else
@maximum = gunCount * 2
endif

~ long range scanner, non-Elysian
elseif (tagis[unitid.iaIgScan] = 1) then
if (tagis[ruleset.ig_sr_elys] < 1) then
gunCount = gunCount
@maximum = gunCount

else
gunCount = gunCount
@maximum = gunCount * 4

endif

~ IA1 Earthshaker Platform
elseif (tagis[iaHelper.iaNotInc] = 1) then
~ now triple for 3 guardsmen per gun
gunCount = gunCount * 3
@maximum = gunCount * 2
@start = gunCount

else
~ heavy artillery
~ now quad for 4 guardsmen per gun
gunCount = gunCount * 4

if (tagis[ruleset.ig_sr_deat] = 1) then
@maximum = gunCount * 1.75
else
@maximum = gunCount * 2

endif
endif

~ now set min/max
@minimum = gunCount]]></sizelimit>
    </option>
